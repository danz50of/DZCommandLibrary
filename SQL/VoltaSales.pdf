select  
substr(h.invoice_id,1,10)                                PRELIM_INVOICE,  
substr(h.invoice_no,1,10)                               INVOICE_NO,  
d.order_no                                                      Order_num,  
h.identity                                                         Customer,   
d.item_id                                                         Line_ID,   
h.invoice_date                                                Inv_Date,  
substr(d.catalog_no,1,16)                               PART,  
d.company                                                      Company,  
substr(IFSAPP.get_prop_code(h.identity,d.company),1,5)   Prop_Code,  
d.description,  
d.invoiced_qty                                                      QTY,  
d.customer_po_no                                          cust_po_no, 
round(d.sale_unit_price,3)                                     Unit_Price,  
substr(IFSAPP.Get_Salesman(h.identity),1,10)      Salesman,  
substr(IFSAPP.Get_Ledger(h.invoice_id,d.item_id),1,10)   Ledger,  
substr(IFSAPP.Get_Product_Line(d.catalog_no,d.contract),1,10)                product_Line,  
substr(IFSAPP.Get_Territory(h.invoice_id,d.item_id),1,10)  Territory,  
round(d.net_curr_amount * h.curr_rate,2)                            SALE_AMT,  
substr(IFSAPP.Get_Order_Part_Cost(d.order_no,d.line_no,d.release_no,d.line_item_no),1,10)   Part_Cost,  
substr(IFSAPP.Get_Order_Part_Cost(d.order_no,d.line_no,d.release_no,d.line_item_no),1,10) * d.invoiced_qty   Ext_Part_Cost,  
substr(IFSAPP.Get_Est_Mat_Costb(d.contract,d.catalog_no),1,10) E_Material_Cost,  
substr(IFSAPP.Get_Labor_Costb(d.contract,d.catalog_no),1,10)  Labor_Cost,  
substr(IFSAPP.Get_Labor_Overhead_Costb(d.contract,d.catalog_no),1,10)         Labor_Overhead_Cost,  
d.contract                 Site,  
h.objstate                  Status,  
substr(IFSAPP.Get_Market_Code(d.order_no),1,10)  Market_Code,  
substr(IFSAPP.Get_Cust_Grp(h.identity),1,10)   Stat_Grp,  
substr(IFSAPP.Get_District(h.identity),1,10)   District,  
substr(IFSAPP.Get_Pricelist(d.order_no,d.item_id),1,10) Pricelist, 
h.curr_rate  Curr_Rate,  
k.name, 
m.address_id, 
m.state, 
p.address1  add1, 
p.address2  add2, 
p.city city, 
p.state  st, 
p.zip_code  zip, 
p.country_code country,
case(p.state)
/*IF ISNULL({PRODUCT_COMMISS_IAL.ZIP})
THEN "NORTH CENTRAL - GREAT LAKES"
ELSE*/
IF {"SD" OR {PRODUCT_COMMISS_IAL.ST} = "ND" OR
   {PRODUCT_COMMISS_IAL.ST} = "MO" OR {PRODUCT_COMMISS_IAL.ST} = "IA" OR 
   {PRODUCT_COMMISS_IAL.ST} = "NE" OR {PRODUCT_COMMISS_IAL.ST} = "KS" OR 
   {PRODUCT_COMMISS_IAL.ST} = "UT" OR {PRODUCT_COMMISS_IAL.ST} = "CO" 
   THEN "CENTRAL PLAINS"
ELSE
IF {PRODUCT_COMMISS_IAL.ST} = "IN" OR {PRODUCT_COMMISS_IAL.ST} = "MI" OR
   {PRODUCT_COMMISS_IAL.ST} = "OH" OR {PRODUCT_COMMISS_IAL.ST} = "KY" OR
   {PRODUCT_COMMISS_IAL.ST} = "MN" OR {PRODUCT_COMMISS_IAL.ST} = "WI"
   THEN "NORTH CENTRAL - GREAT LAKES"
ELSE 
IF {PRODUCT_COMMISS_IAL.ST} = "MA" OR {PRODUCT_COMMISS_IAL.ST} = "CT" OR
   {PRODUCT_COMMISS_IAL.ST} = "NH" OR {PRODUCT_COMMISS_IAL.ST} = "ME" OR
   {PRODUCT_COMMISS_IAL.ST} = "VT" OR {PRODUCT_COMMISS_IAL.ST} = "RI" OR 
   {PRODUCT_COMMISS_IAL.ST} = "ON" OR {PRODUCT_COMMISS_IAL.ST} = "PE" OR 
   {PRODUCT_COMMISS_IAL.ST} = "QC" OR {PRODUCT_COMMISS_IAL.ST} = "NB" OR 
   {PRODUCT_COMMISS_IAL.ST} = "NL" OR {PRODUCT_COMMISS_IAL.ST} = "NS" OR 
   {PRODUCT_COMMISS_IAL.ST} = "NY" 
 THEN "NEW ENGLAND"
ELSE
IF 
   {PRODUCT_COMMISS_IAL.ST} = "MD" OR {PRODUCT_COMMISS_IAL.ST} = "VA" OR
   {PRODUCT_COMMISS_IAL.ST} = "WV" OR {PRODUCT_COMMISS_IAL.ST} = "DC" OR
   {PRODUCT_COMMISS_IAL.ST} = "DE" OR {PRODUCT_COMMISS_IAL.ST} = "NJ" OR
   {PRODUCT_COMMISS_IAL.ST} = "PA"
   THEN "MID ATLANTIC"
ELSE
IF {PRODUCT_COMMISS_IAL.ST} = "OR" OR {PRODUCT_COMMISS_IAL.ST} = "WA" OR
   {PRODUCT_COMMISS_IAL.ST} = "AK" OR {PRODUCT_COMMISS_IAL.ST} = "MT" OR
   {PRODUCT_COMMISS_IAL.ST} = "WY" OR {PRODUCT_COMMISS_IAL.ST} = "ID" OR
   {PRODUCT_COMMISS_IAL.ST} = "BC" OR {PRODUCT_COMMISS_IAL.ST} = "AB" OR
   {PRODUCT_COMMISS_IAL.ST} = "SK" OR {PRODUCT_COMMISS_IAL.ST} = "MB" OR
   {PRODUCT_COMMISS_IAL.ST} = "YT" OR {PRODUCT_COMMISS_IAL.ST} = "NT" OR
   {PRODUCT_COMMISS_IAL.ST} = "NU" 
   THEN "NORTHWEST"
ELSE 
IF {PRODUCT_COMMISS_IAL.ST} = "LA" OR {PRODUCT_COMMISS_IAL.ST} = "OK" OR
   {PRODUCT_COMMISS_IAL.ST} = "TX" OR {PRODUCT_COMMISS_IAL.ST} = "MS" OR
   {PRODUCT_COMMISS_IAL.ST} = "AR" OR {PRODUCT_COMMISS_IAL.ST} = "NM"
   THEN "SOUTH"
ELSE 
IF {PRODUCT_COMMISS_IAL.ST} = "AL" OR {PRODUCT_COMMISS_IAL.ST} = "FL" OR
   {PRODUCT_COMMISS_IAL.ST} = "GA" OR {PRODUCT_COMMISS_IAL.ST} = "SC" OR 
   {PRODUCT_COMMISS_IAL.ST} = "NC" OR{PRODUCT_COMMISS_IAL.ST} = "PR" OR
   {PRODUCT_COMMISS_IAL.ST} = "TN" 
   THEN "SOUTHEAST"
ELSE
IF {PRODUCT_COMMISS_IAL.ST} = "AZ" OR {PRODUCT_COMMISS_IAL.ST} = "HI" OR
   {PRODUCT_COMMISS_IAL.ST} = "NV"     
   THEN "SOUTHWEST"
ELSE IF
   {PRODUCT_COMMISS_IAL.ST} = "CA" OR 
   {PRODUCT_COMMISS_IAL.ST} = "IL" 
THEN IF NUMERICTEXT (Left ({PRODUCT_COMMISS_IAL.ZIP},5))
   THEN IF ToNumber  
  (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) >= 60000 AND
           ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) <= 61199
           THEN "NORTH CENTRAL - GREAT LAKES"
   ELSE IF ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) >= 93301 AND
           ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) <= 96699
           THEN "NORTHWEST"  
   ELSE IF ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) >= 61200 AND
           ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) <= 62999
           THEN "CENTRAL PLAINS"  
   ELSE IF ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) >= 90000 AND
           ToNumber (Left ({PRODUCT_COMMISS_IAL.ZIP},5)) <= 93300
           THEN "SOUTHWEST"
   ELSE "INVALID NY,IL,CA,"
ELSE "NON NUMERIC"
ELSE "*CHANGE TO HEADQUARTERED REGION*"
from  
IFSAPP.customer_order_inv_item d, 
IFSAPP.customer_order_inv_head h, 
IFSAPP.customer_info k, 
IFSAPP.customer_info_address m, 
IFSAPP.customer_order_address p 
where  
     d.invoice_id = h.invoice_id and  
     d.identity = h.identity and  
     h.objstate <> 'Preliminary' and 
     d.identity = k.customer_id and 
     d.identity = m.customer_id and 
     m.customer_id = '907148' and
     d.identity = p.customer_no and  
     d.order_no = p.order_no and 
     h.delivery_address_id = m.address_id 
